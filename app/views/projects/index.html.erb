<% if user_signed_in? %>

  <div class= 'mt-3'>
    <h3 class='ml-5' >Projects List</h3>
  </div>

  <div class='container-fluid'>
    <div class='mt-3 row justify-content-center' style="column-gap:20px; row-gap:20px;">
    <% if Project.count > 0%>
      <%= render @projects %>
    <% end %>
    </div>
    <div class='d-flex justify-content-center position-static mt-5'>
      <% if Project.count > 0 %>
        <%= paginate @projects %>
      <% end %>
    </div>
  </div>

<% end %>
